<section>
  <h2>Crie sua própria sala</h2>
  <form action="/create-room" method="POST">
    <label class="sr-only" for="room-pass">Insira uma senha</label>
    <!--label invisível para o navegador, porém escondido no navegador -Acessibilidade-->
    <input
      type="password"
      name="password"
      id="room-pass"
      placeholder="Insira uma senha"
    />
    <button>
      <img src="images/users-white.svg" alt="Entrar na sala" />
      Criar sala
    </button>
  </form>
</section>
